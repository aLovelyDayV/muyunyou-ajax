<ul class="clearfix">
    {{each items}}
    <li>
        <a href="#">
            <div class="pic">
                <img src="{{$value.url}}" alt="">
            </div>
            <h4>{{$value.title}}</h4>
            <p><i>★{{$value.score}}</i>（{{$value['comment_counts']}}评价) {{$value['join_counts']}}人参加过</p>
        </a>
    </li>
    {{/each}}
</ul>